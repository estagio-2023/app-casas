CREATE TABLE tipologia
(id SERIAL  PRIMARY KEY  NOT NULL ,
descricao varchar(50) );

CREATE TABLE comodidades
(id SERIAL  PRIMARY KEY  NOT NULL ,
descricao varchar(50) );

CREATE TABLE cidades
(id SERIAL  PRIMARY KEY  NOT NULL ,
descricao varchar(50) );

CREATE TABLE tipo_imovel
(id SERIAL  PRIMARY KEY  NOT NULL ,
descricao varchar(50) );


